{
  "name": "React-Sandbox",
  "version": "0.0.0",
  "private": true,
  "main": "bin/www",
  "scripts": {
    "start": "nodemon .",
    "webpack": "NODE_PATH=NODE_PATH:./src/shared:./src/server webpack-dev-server --no-info --config webpack.config.js",
    "build": "NODE_ENV=production NODE_PATH=NODE_PATH:./src/shared:./src/server webpack --config webpack.config.production.js --progress --profile --color -p",
    "lint": "NODE_PATH=NODE_PATH:./src/shared:./src/server  eslint src",
    "debug": "NODE_PATH=NODE_PATH:./src/shared:./src/server node-debug bin/www",
    "build-node": "babel -s inline -D src/ -d dist",
    "server:start": "NODE_ENV=production NODE_PATH=NODE_PATH:./dist/shared:./dist/server node dist/server"
  },
  "dependencies": {
    "babel-register": "^6.6.5",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "draft-js": "^0.1.0",
    "estraverse": "4.2.0",
    "estraverse-fb": "^1.3.1",
    "express": "~4.13.1",
    "helmet": "^1.3.0",
    "intl": "^1.1.0",
    "log4js": "^0.6.32",
    "morgan": "~1.6.1",
    "multer": "^1.1.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-intl": "^1.2.2",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "redux": "^3.3.1",
    "serve-favicon": "~2.3.0",
    "source-map-support": "^0.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.6.5",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-intl": "^2.1.1",
    "babel-plugin-syntax-object-rest-spread": "^6.3.13",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.3.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",
    "nodemon": "^1.9.1",
    "postcss-loader": "^0.8.1",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "toolbox-loader": "0.0.3",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
