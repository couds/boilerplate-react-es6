{
  "name": "react-boilerplate",
  "version": "0.0.0",
  "private": true,
  "main": "bin/www",
  "scripts": {
    "start": "NODE_PATH=NODE_PATH:./src/shared:./src/server nodemon .",
    "webpack": "webpack --watch",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.js --progress --profile --color",
    "build:node": "babel -s inline -D src/ -d dist",
    "build": "npm run build:node && npm run build:webpack",
    "lint": "NODE_PATH=NODE_PATH:./src/shared:./src/server  eslint --fix src",
    "debug": "NODE_PATH=NODE_PATH:./src/shared:./src/server node-debug bin/www",
    "server:start": "NODE_ENV=production NODE_PATH=NODE_PATH:./dist/shared:./dist/server node dist/server"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.0",
    "babel-plugin-react-intl": "2.3.1",
    "babel-plugin-syntax-object-rest-spread": "6.13.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.24.1",
    "chai": "4.0.2",
    "compression-webpack-plugin": "^0.4.0",
    "css-loader": "0.28.4",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-plugin-import": "2.5.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "2.1.2",
    "mocha": "3.4.2",
    "node-sass": "4.5.3",
    "nodemon": "1.11.0",
    "postcss-loader": "2.0.6",
    "progress-bar-webpack-plugin": "^1.9.3",
    "sass-loader": "6.0.6",
    "source-map-support": "0.4.15",
    "style-loader": "0.18.2",
    "toolbox-loader": "0.0.3",
    "webpack": "2.2.0"
  },
  "dependencies": {
    "body-parser": "1.17.2",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "debug": "2.6.8",
    "express": "4.15.3",
    "helmet": "3.21.1",
    "immutable": "3.8.1",
    "intl": "1.2.5",
    "log4js": "1.1.1",
    "morgan": "1.8.2",
    "multer": "1.3.0",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-intl": "2.3.0",
    "react-redux": "5.0.5",
    "react-router": "3.0.5",
    "redux": "3.7.0",
    "serve-favicon": "2.4.3"
  }
}
