{
  "name": "React-Boilerplate",
  "version": "0.0.0",
  "private": true,
  "main": "bin/www",
  "scripts": {
    "start": "NODE_ENV=production NODE_PATH=NODE_PATH:./dist/shared:./dist/server node dist/server",
    "start:dev": "nodemon .",
    "webpack": "NODE_PATH=NODE_PATH:./src/shared:./src/server webpack-dev-server --no-info --config webpack.config.js",
    "build:webpack": "NODE_ENV=production NODE_PATH=NODE_PATH:./dist/shared:./dist/server webpack --config webpack.config.production.js --progress --profile --color -p",
    "build:node": "babel -s inline -D src/ -d dist",
    "build": "npm run build:node && npm run build:webpack",
    "lint": "NODE_PATH=NODE_PATH:./src/shared:./src/server  eslint src"
  },
  "dependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-plugin-react-intl": "^2.1.3",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.0",
    "babel-register": "^6.9.0",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "debug": "*",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-react": "^5.2.2",
    "express": "^4.14.0",
    "helmet": "^2.1.1",
    "intl": "^1.2.4",
    "mocha": "^2.5.3",
    "morgan": "^1.7.0",
    "nodemon": "^1.9.2",
    "postcss-loader": "^0.9.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-intl": "^2.1.3",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.1",
    "redux": "^3.5.2",
    "sass-loader": "^4.0.0",
    "serve-favicon": "*",
    "source-map-support": "*",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "babel-cli": "*",
    "babel-core": "*",
    "babel-eslint": "*",
    "babel-loader": "*",
    "babel-plugin-react-intl": "*",
    "babel-plugin-syntax-object-rest-spread": "*",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-object-assign": "*",
    "babel-polyfill": "*",
    "babel-preset-es2015": "*",
    "babel-preset-react": "*",
    "babel-preset-stage-0": "*",
    "babel-register": "*",
    "chai": "*",
    "css-loader": "*",
    "eslint": "*",
    "eslint-config-airbnb": "*",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "*",
    "extract-text-webpack-plugin": "*",
    "mocha": "*",
    "node-sass": "*",
    "nodemon": "*",
    "postcss-loader": "*",
    "sass-loader": "*",
    "style-loader": "*",
    "toolbox-loader": "*",
    "webpack": "*",
    "webpack-dev-server": "*"
  }
}
